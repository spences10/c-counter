{
  "name": "c-counter",
  "version": "0.0.1",
  "description": "Pick foods to a calorie budget",
  "devDependencies": {
    "autoprefixer-stylus": "0.13.0",
    "babel-eslint": "^7.1.1",
    "concurrently": "3.4.0",
    "eslint": "^3.12.2",
    "eslint-plugin-flowtype": "^2.33.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "react-scripts": "0.12.2",
    "stylus": "0.54.5"
  },
  "dependencies": {
    "history": "4.6.1",
    "re-base": "2.7.1",
    "react": "15.5.4",
    "react-addons-css-transition-group": "15.5.2",
    "react-dom": "15.5.4",
    "react-router": "4.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "watch": "concurrently --names \"webpack, stylus\" --prefix name \"npm run start\" \"npm run styles:watch\"",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "styles": "stylus -u autoprefixer-stylus ./src/css/style.styl -o ./src/css/style.css",
    "styles:watch": "stylus -u autoprefixer-stylus -w ./src/css/style.styl -o ./src/css/style.css"
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/.eslintrc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spences10/fast-diet-picker.git"
  },
  "keywords": [
    "diet",
    "calories",
    "picker",
    "planner"
  ],
  "author": "Scott Spence",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spences10/fast-diet-picker/issues"
  },
  "homepage": "https://github.com/spences10/fast-diet-picker#readme"
}
