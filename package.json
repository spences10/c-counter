{
  "name": "c-counter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "0.17.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.0",
    "styled-components": "2.4.0"
  },
  "devDependencies": {
    "eslint": "4.15.0",
    "eslint-plugin-react": "7.5.1",
    "husky": "0.14.3",
    "prettier": "^1.8.2",
    "pretty-quick": "1.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && mv build c-counter",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "format": "pretty-quick",
    "precommit": "pretty-quick --staged",
    "clean": "rm -rf build/ && rm -rf c-counter/",
    "deploy:now": "npm run clean && npm run build && now c-counter/",
    "alias:dev": "now alias c-counter.now.sh --token $NOW_TOKEN",
    "alias:prod": "now alias c-counter.scottspence.me --token $NOW_TOKEN",
    "cleanup": "now rm c-counter --safe --yes --token $NOW_TOKEN"
  },
  "pre-commit": "npm run format",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spences10/c-counter.git"
  },
  "keywords": [],
  "author": "Scott Spence <spences10apps@gmail.com> (https://blog.scottspence.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spences10/c-counter/issues"
  },
  "homepage": "https://c-counter.now.sh",
  "description": ""
}
