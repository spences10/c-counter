{
  "name": "c-counter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "0.18.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "5.2.0",
    "react-scripts": "1.1.3",
    "styled-components": "3.2.1"
  },
  "devDependencies": {
    "eslint": "4.18.1",
    "eslint-plugin-react": "7.7.0",
    "husky": "0.14.3",
    "prettier": "1.11.0",
    "pretty-quick": "1.4.1",
    "release-it": "7.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && mv build c-counter",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "format": "pretty-quick",
    "precommit": "pretty-quick --staged",
    "clean": "rm -rf build/ && rm -rf c-counter/",
    "deploy:now": "now c-counter/ --token $NOW_TOKEN",
    "alias:dev": "now alias c-counter.now.sh --token $NOW_TOKEN",
    "alias:prod":
      "now alias c-counter.scottspence.me --token $NOW_TOKEN",
    "cleanup": "now rm c-counter --safe --yes --token $NOW_TOKEN",
    "release": "release-it"
  },
  "pre-commit": "npm run format",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spences10/c-counter.git"
  },
  "keywords": [],
  "author":
    "Scott Spence <spences10apps@gmail.com> (https://scottspence.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spences10/c-counter/issues"
  },
  "homepage": "https://c-counter.now.sh",
  "description": ""
}
