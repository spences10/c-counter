{
  "name": "c-counter",
  "version": "0.0.1",
  "description": "Pick foods to a calorie budget",
  "devDependencies": {
    "autoprefixer-stylus": "0.14.0",
    "babel-eslint": "^8.0.2",
    "concurrently": "3.5.0",
    "eslint": "^4.10.0",
    "eslint-plugin-flowtype": "^2.38.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "react-scripts": "1.0.17",
    "stylus": "0.54.5"
  },
  "dependencies": {
    "re-base": "3.0.5",
    "history": "4.7.2",
    "react-addons-css-transition-group": "15.6.2",
    "react": "16.1.0",
    "react-dom": "16.1.0",
    "react-router": "4.2.0",
    "snyk": "^1.49.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "watch": "concurrently --names \"webpack, stylus\" --prefix name \"npm run start\" \"npm run styles:watch\"",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "styles": "stylus -u autoprefixer-stylus ./src/css/style.styl -o ./src/css/style.css",
    "styles:watch": "stylus -u autoprefixer-stylus -w ./src/css/style.styl -o ./src/css/style.css",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/.eslintrc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spences10/fast-diet-picker.git"
  },
  "keywords": [
    "diet",
    "calories",
    "picker",
    "planner"
  ],
  "author": "Scott Spence",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spences10/fast-diet-picker/issues"
  },
  "homepage": "https://github.com/spences10/fast-diet-picker#readme",
  "snyk": true
}